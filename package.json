{
  "name": "create-next-auth-app",
  "version": "14.1.4",
  "keywords": [
    "react",
    "next",
    "next.js",
    "next-auth",
    "auth.js"
  ],
  "description": "Create a Next.js app with authentication using Auth.js",
  "author": "Burak Can <burak@burakcan.me>",
  "bin": {
    "create-next-app": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "ncc build ./index.ts -w -o dist/",
    "release": "ncc build ./index.ts -o ./dist/ --minify --no-cache --no-source-map-register",
    "build": "pnpm release",
    "lint-fix": "pnpm prettier -w --plugin prettier-plugin-tailwindcss 'templates/*-tw/{ts,js}/{app,pages}/**/*.{js,ts,tsx}'"
  },
  "devDependencies": {
    "@types/async-retry": "1.4.2",
    "@types/ci-info": "2.0.0",
    "@types/cross-spawn": "6.0.0",
    "@types/node": "^20.2.5",
    "@types/prompts": "2.4.2",
    "@types/tar": "6.1.5",
    "@types/validate-npm-package-name": "3.0.0",
    "@vercel/ncc": "0.34.0",
    "async-sema": "^3.1.1",
    "commander": "2.20.0",
    "cross-spawn": "7.0.3",
    "fast-glob": "3.3.1",
    "picocolors": "1.0.0",
    "prettier-plugin-tailwindcss": "0.3.0",
    "prompts": "2.4.2",
    "tar": "6.1.15",
    "typescript": "^5.4.3",
    "update-check": "1.5.4",
    "validate-npm-package-name": "3.0.0"
  },
  "engines": {
    "node": ">=18.17.0"
  }
}
